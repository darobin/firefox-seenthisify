{
  "manifest_version": 2,
  "name": "SeenThisify",
  "version": "1.1",
  "description": "Use this to post content to SeenThis.net easily",
  "homepage_url": "https://github.com/darobin/seenthisify",
  "icons": {
    "48": "icons/seenthis-48.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/seenthis.svg",
    "default_title": "SeenThisify"
  },
  "background": {
    "scripts": ["seenthisify.js"]
  }
}
